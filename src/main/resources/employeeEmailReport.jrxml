<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="employeeReport" pageWidth="612" pageHeight="792" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<parameter name="idLocation" class="java.lang.Integer" isForPrompting="false"/>
	<queryString>
		<![CDATA[
			select *
			FROM history_records hr
			INNER JOIN user_groups ug ON hr.group_id = ug.id
			WHERE location_id = $P{idLocation}
			LIMIT 5
		]]>
	</queryString>
	<field name="NAME" class="java.lang.String"/>
	<detail>
		<band height="20" splitType="Stretch">
			<textField>
				<reportElement x="0" y="0" width="500" height="20"/>
				<textElement>
					<font fontName="Tahoma"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$V{REPORT_COUNT} + ") " + $F{NAME}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
